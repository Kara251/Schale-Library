{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "推荐作品",
    "description": "推荐作品管理，支持RSS自动导入"
  },
  "options": {
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "author": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "nature": {
      "type": "enumeration",
      "enum": [
        "official",
        "fanmade"
      ],
      "default": "fanmade",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "workType": {
      "type": "enumeration",
      "enum": [
        "video",
        "image",
        "text",
        "other"
      ],
      "default": "video",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "link": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "sourceUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "sourcePlatform": {
      "type": "enumeration",
      "enum": [
        "bilibili",
        "twitter",
        "pixiv",
        "youtube",
        "other",
        "manual"
      ],
      "default": "manual",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "sourceId": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isAutoImported": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "importedAt": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student.student",
      "inversedBy": "works"
    },
    "coverImageUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "originalPublishDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}